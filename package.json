{
  "name": "logr",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "bun .output/server/index.mjs",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check --write --unsafe --files-ignore-unknown=true",
    "prepare": "husky",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:delete": "docker compose down -v",
    "db:psql": "docker exec -it leaf-pg psql -U user -d leaf",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "bun run db:generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "@tanstack/nitro-v2-vite-plugin": "^1.133.19",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-router": "^1.134.4",
    "@tanstack/react-router-ssr-query": "^1.134.4",
    "@tanstack/react-start": "^1.134.7",
    "better-auth": "^1.3.34",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@tailwindcss/vite": "^4.1.16",
    "@types/bun": "^1.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
